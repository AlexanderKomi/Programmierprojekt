<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<HBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:id="hbox_1" prefHeight="800.0" prefWidth="1200.0"
      >
    <children>
        <VBox fx:id="vbox_1" alignment="CENTER" prefHeight="800.0" prefWidth="1200.0" HBox.hgrow="ALWAYS">
            <children>
                <Pane prefHeight="200.0" prefWidth="200.0"/>
                <ImageView fx:id="pic_header" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true"
                           preserveRatio="true"/>
                <Pane prefHeight="200.0" prefWidth="200.0"/>
                <Button fx:id="b_play" mnemonicParsing="false" onAction="#button_clicked" text="play"/>
                <Pane prefHeight="200.0" prefWidth="200.0"/>
                <Button fx:id="b_tutorial" mnemonicParsing="false" onAction="#button_clicked" text="tutorial"/>
                <Pane prefHeight="200.0" prefWidth="200.0"/>
                <Button fx:id="b_exit" mnemonicParsing="false" onAction="#button_clicked" text="exit"/>
                <Pane prefHeight="200.0" prefWidth="200.0"/>
            </children>
        </VBox>
    </children>
    <stylesheets>
        <URL value="@resources/dimensions.css"/>
        <URL value="@resources/mainMenu.css"/>
    </stylesheets>
</HBox>
