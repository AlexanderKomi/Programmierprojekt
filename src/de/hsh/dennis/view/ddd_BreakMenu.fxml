<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import java.net.URL?>
<HBox xmlns:fx="http://javafx.com/fxml/1" fx:id="hbox_1" prefHeight="100.0" prefWidth="200.0"
      xmlns="http://javafx.com/javafx/10.0.1">
    <children>
        <VBox fx:id="vbox_1" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
            <children>
                <Pane prefHeight="200.0" prefWidth="200.0"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="[ SCORE ]">
                    <font>
                        <Font size="42.0"/>
                    </font>
                </Text>
                <TextField fx:id="tf_score" alignment="CENTER" editable="false" text="0">
                    <VBox.margin>
                        <Insets bottom="20.0" left="300.0" right="300.0" top="20.0"/>
                    </VBox.margin>
                </TextField>
                <Pane prefHeight="200.0" prefWidth="200.0"/>
                <Button fx:id="b_play" mnemonicParsing="false" onAction="#button_clicked" text="replay"/>
                <Pane prefHeight="200.0" prefWidth="200.0"/>
                <Button fx:id="b_tutorial" mnemonicParsing="false" onAction="#button_clicked" text="main menu"/>
                <Pane prefHeight="200.0" prefWidth="200.0"/>
                <Button fx:id="b_exit" mnemonicParsing="false" onAction="#button_clicked" text="continue"/>
                <Pane prefHeight="200.0" prefWidth="200.0"/>
            </children>
        </VBox>
    </children>
    <stylesheets>
        <URL value="@../../../../../../gui/gui_prototyp_game/dimensions.css"/>
        <URL value="@../../../../../../gui/gui_prototyp_game/main.css"/>
    </stylesheets>
</HBox>
