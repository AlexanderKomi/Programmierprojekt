<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.TextFlow?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:id="basicPane" alignment="CENTER">
    <TextFlow textAlignment="CENTER">
        <Label text="Spielname"/>
    </TextFlow>
    <HBox alignment="CENTER" VBox.vgrow="SOMETIMES">
        <VBox alignment="CENTER" spacing="10.0" HBox.hgrow="SOMETIMES">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <Label text="Spieler 1"/>
            <Label text="Steuerung"/>
        </VBox>
        <VBox alignment="CENTER" spacing="10.0" HBox.hgrow="SOMETIMES">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <Label text="Anleitung"/>
        </VBox>
        <VBox alignment="CENTER" spacing="10.0" HBox.hgrow="SOMETIMES">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <Label text="Spieler 2"/>
            <Label text="Steuerung"/>
        </VBox>
    </HBox>
    <HBox alignment="CENTER" spacing="10.0">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <Button fx:id="backButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                onAction="#backButtonPressed"
                text="zurÃ¼ck"/>
        <Button fx:id="okButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                onAction="#okButtonPressed"
                text="OK"/>
    </HBox>
</VBox>
