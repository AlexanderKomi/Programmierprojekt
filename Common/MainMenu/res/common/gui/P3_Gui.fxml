<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.net.URL?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:id="vbox" alignment="CENTER" maxHeight="800" maxWidth="1200"
      minHeight="800" minWidth="1200" xmlns="http://javafx.com/javafx"
      fx:controller="common.MainMenu">
   <AnchorPane fx:id="ap_desktop" VBox.vgrow="ALWAYS">
      <children>
         <GridPane alignment="CENTER" hgap="60.0" layoutX="90.0" layoutY="90.0" vgap="60.0">
           <columnConstraints>
             <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
             <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
           </columnConstraints>
           <rowConstraints>
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
           </rowConstraints>
            <children>
               <VBox alignment="CENTER" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                   <Button fx:id="b_game_1" mnemonicParsing="false" />
                   <Text fx:id="txt_game_1" strokeType="OUTSIDE" strokeWidth="0.0" text="Spiel 1" />
               </VBox>
               <VBox alignment="CENTER" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                   <Button fx:id="b_game_2" mnemonicParsing="false" />
                   <Text fx:id="txt_game_2" strokeType="OUTSIDE" strokeWidth="0.0" text="Spiel 2" />
               </VBox>
               <VBox alignment="CENTER" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                   <Button fx:id="b_game_4" mnemonicParsing="false" />
                   <Text fx:id="txt_game_4" strokeType="OUTSIDE" strokeWidth="0.0" text="Spiel 4" />
               </VBox>
               <VBox alignment="CENTER" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                   <Button fx:id="b_game_3" mnemonicParsing="false" />
                   <Text fx:id="txt_game_3" strokeType="OUTSIDE" strokeWidth="0.0" text="Spiel 3" />
               </VBox>
               <VBox alignment="CENTER" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                   <Button fx:id="b_game_5" mnemonicParsing="false" />
                   <Text fx:id="txt_game_5" strokeType="OUTSIDE" strokeWidth="0.0" text="Spiel 5" />
               </VBox>
               <VBox alignment="CENTER" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                   <Button fx:id="b_game_6" mnemonicParsing="false" />
                   <Text fx:id="txt_game_6" strokeType="OUTSIDE" strokeWidth="0.0" text="Spiel 6" />
               </VBox>
            </children>
         </GridPane>
      </children>
   </AnchorPane>
   <HBox fx:id="hbox_bar" prefHeight="40.0" VBox.vgrow="NEVER">
       <Button fx:id="b_shutdown" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0" HBox.hgrow="SOMETIMES" />
       <ComboBox fx:id="cb_credits" prefHeight="40.0" prefWidth="150.0" promptText="Credits" />
   </HBox>
   <stylesheets>
       <URL value="@main.java.main.css" />
       <URL value="@dimensions.css" />
   </stylesheets>
</VBox>
