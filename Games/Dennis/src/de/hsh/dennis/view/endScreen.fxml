<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox fx:id="hbox_1" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox fx:id="vbox_1" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
            <children>
                <Pane prefHeight="200.0" prefWidth="200.0" />
                <Text fx:id="t_banner" strokeType="OUTSIDE" strokeWidth="0.0" text="[ SCORE ]">
                    <font>
                        <Font size="42.0" />
                    </font>
                </Text>
                <TextField fx:id="tf_score" alignment="CENTER" editable="false" text="0">
                    <VBox.margin>
                        <Insets bottom="20.0" left="300.0" right="300.0" top="20.0" />
                    </VBox.margin>
                </TextField>
                <Pane prefHeight="200.0" prefWidth="200.0" />
                <Button fx:id="b_replay" mnemonicParsing="false" onAction="#button_clicked" text="replay" />
                <Pane prefHeight="200.0" prefWidth="200.0" />
                <Button fx:id="b_main_menu" mnemonicParsing="false" onAction="#button_clicked" text="main.java.main menu" />
                <Pane prefHeight="200.0" prefWidth="200.0" />
                <Pane prefHeight="200.0" prefWidth="200.0" />
            </children>
        </VBox>
    </children>
    <stylesheets>
        <URL value="@css/dimensions.css" />
    </stylesheets>
</HBox>
